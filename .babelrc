{
  "comments": false,
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ],
        "@babel/preset-react"
      ]
    },
    "commonjs": {
      "plugins": [
        "add-module-exports",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-object-rest-spread"
      ]
    }
  }
}
